{% extends 'default/objectListPage.html.twig' %}

{% block table %}
	
	<table id="patients_table" class="table table-striped table-bordered">
		<thead>
	        <tr>
	        	<th>Id</th>
	            <th>Name</th>
	            <th>E-Mail</th>
	            <th>Tel. Nummer</th>
	        </tr>
	    </thead>

	    {% for p in patients %}
	        <tr>
	        	<td>{{ p.id }}</td>
	            <td>{{ p.firstName ~ " " ~ p.lastName }}</td>
	            <td>{{ p.email }}</td>
	            <td>{{ p.phoneNumber }}</td>
	        </tr>
	    {% endfor %}
    </table>

{% endblock %}
